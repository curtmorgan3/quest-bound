{
  "name": "compass-server",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "build": "npx tsc && npm run gql:copySchema && npm run db:generate && tsc-alias",
    "start": "node dist/app.js",
    "dev": "nodemon",
    "minify": "rollup dist/app.js --file dist/bundle.js --format cjs",
    "forwardWebhook": "stripe listen --forward-to localhost:8000/payment-webhook",
    "gql:copySchema": "cp src/infrastructure/graphql/schema.graphql dist/infrastructure/graphql/schema.graphql",
    "gql:generate": "cd src/infrastructure/graphql && amplify codegen && cd ../../..",
    "gql:copyTypes": "rm /Users/curtmorgan/workshop/qbt/applications/compass-web-app/src/libs/compass-api/gql/generated-types.ts && cat src/infrastructure/graphql/generated-types.ts >> /Users/curtmorgan/workshop/qbt/applications/compass-web-app/src/libs/compass-api/gql/generated-types.ts",
    "db:generate": "prisma generate --schema=./src/database/prisma/schema.prisma",
    "db:migrate:local": "npm run env:local && prisma migrate dev --schema=./src/database/prisma/schema.prisma",
    "db:migrate:dev": "npm run env:dev && prisma migrate dev --schema=./src/database/prisma/schema.prisma && npm run env:local",
    "db:migrate:prod": "npm run env:prod && prisma migrate dev --schema=./src/database/prisma/schema.prisma && npm run env:local",
    "env:local": "rm .env && cat local.env.txt >> .env",
    "env:dev": "rm .env && cat dev.env.txt >> .env",
    "env:prod": "rm .env && cat prod.env.txt >> .env"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "node -r tsconfig-paths/register -r ts-node/register ./src/app.ts",
    "ext": "ts, js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apollo/server": "^4.10.0",
    "@aws-sdk/client-sesv2": "^3.515.0",
    "@graphql-tools/schema": "^10.0.4",
    "@sentry/node": "^7.107.0",
    "@supabase/supabase-js": "^2.39.3",
    "@types/cors": "^2.8.17",
    "@types/express-rate-limit": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/stripe": "^8.0.417",
    "@types/uuid": "^9.0.8",
    "axios": "^1.6.7",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "dot-env": "^0.0.1",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "graphql": "^16.8.1",
    "graphql-redis-subscriptions": "^2.6.1",
    "graphql-subscriptions": "^2.0.0",
    "graphql-ws": "^5.16.0",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "node-cache": "^5.1.2",
    "pdf.js-extract": "^0.2.1",
    "pdfreader": "^3.0.2",
    "rollup": "^4.12.0",
    "stripe": "^14.18.0",
    "tsconfig-paths": "^4.2.0",
    "uuid": "^9.0.1",
    "ws": "^8.17.0"
  },
  "devDependencies": {
    "@prisma/client": "^5.9.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.4",
    "prisma": "^5.9.0",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.3.3"
  }
}
